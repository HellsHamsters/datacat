{
  "name": "datacat",
  "version": "0.1.0",
  "description": "datacat",
  "main": "build/desktop.bundle.js",
  "scripts": {
    "test": "karma start karma.conf.js --single-run",
    "app": "cross-env NODE_ENV=development electron .",
    "build:dev": "concurrently \"npm run build:dev:desktop\" \"npm run build:dev:browser\" ",
    "build:dev:run": "cross-env NODE_ENV=development webpack --profile --progress --watch --colors",
    "build:dev:desktop": "cross-env BUILD_TARGET=DESKTOP npm run build:dev:run",
    "build:dev:browser": "cross-env BUILD_TARGET=BROWSER npm run build:dev:run",
    "build:dev:dll": "webpack -p --progress --colors --config webpack.dll.config.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "clean": "rimraf build/* && rimraf *.log && rimraf tests-results",
    "clean-pre-build": "npm run clean && npm run build:dev:dll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HellsHamsters/datacat.git"
  },
  "author": {
    "name": "Anton Piskunov",
    "email": "for@a-piskunov.ru",
    "url": "https://github.com/smile42ru"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/HellsHamsters/datacat/issues"
  },
  "homepage": "https://github.com/HellsHamsters/datacat#readme",
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/compiler-cli": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "@types/node": "^6.0.57",
    "@types/webpack": "^2.1.0",
    "electron-context-menu": "^0.8.0",
    "jquery": "^3.1.1",
    "mysql": "^2.12.0",
    "node-notifier": "^4.6.1",
    "select2": "^4.0.3"
  },
  "devDependencies": {
    "codelyzer": "^2.0.0-beta.4",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "ravenjs": "^1.1.1",
    "rxjs": "^5.0.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^1.3.3",
    "tslint": "^4.2.0",
    "typedoc": "^0.5.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.0.0",
    "webpack-notifier": "^1.5.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "zone.js": "^0.7.4"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x"
  }
}
